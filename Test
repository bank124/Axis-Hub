    spawn(function()
            while task.wait() do
                pcall(function()
                    if _G.Notification then
                    repeat
                    wait(60)
    local executeor = tostring((syn and syn.request or request)({Url = 'https://api.luauth.xyz/hwid', Method = syn and "Get" or "GET"}).Body)
    local url = webhook
    local data = {
        ["embeds"] = {
